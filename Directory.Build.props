<Project>

    <PropertyGroup>
        <Version>1.0.0-alpha.1</Version>
        <Authors>Kizari</Authors>
        <Company>Visium</Company>
        <Description>Various utilities for .NET projects.</Description>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <RepositoryUrl>https://github.com/Kizari/Anima.git</RepositoryUrl>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Copyright>Copyright Â© 2024 Kizari</Copyright>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageReleaseNotes>https://github.com/Kizari/Anima/blob/main/CHANGELOG.md</PackageReleaseNotes>
        <RepositoryBranch>main</RepositoryBranch>
        <RepositoryCommit>48a89803a81ccbec5b3467e0b07d6a1b1a0d41f4</RepositoryCommit>
        <PackageTags>source-generators;roslyn</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <None Include="$(MSBuildThisFileDirectory)LICENSE" Pack="true" PackagePath=""/>
        <None Include="$(MSBuildThisFileDirectory)README.md" Pack="true" PackagePath=""/>
        <None Include="$(MSBuildThisFileDirectory)CHANGELOG.md" Pack="true" PackagePath=""/>
    </ItemGroup>

</Project>