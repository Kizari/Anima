<Project>

    <PropertyGroup>
        <Version>1.0.0-alpha.3</Version>
        <Authors>Kizari</Authors>
        <Company>Visium</Company>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <RepositoryUrl>https://github.com/Kizari/Anima.git</RepositoryUrl>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Copyright>Copyright Â© 2024 Kizari</Copyright>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageReleaseNotes>https://github.com/Kizari/Anima/blob/main/CHANGELOG.md</PackageReleaseNotes>
        <RepositoryBranch>main</RepositoryBranch>
        <RepositoryCommit>5410504d4d5928f1b931eac640a6bf393f6c59a8</RepositoryCommit>
        <PackageTags>source-generators;roslyn</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <None Include="$(MSBuildThisFileDirectory)LICENSE" Pack="true" PackagePath=""/>
        <None Include="README.md" Pack="true" PackagePath=""/>
        <None Include="$(MSBuildThisFileDirectory)CHANGELOG.md" Pack="true" PackagePath=""/>
    </ItemGroup>

</Project>